<div class="row">
  <div class="col s12">
    <div class="divider"></div>
  </div>
</div>

<div class="row" id="degree-title" data-degree-id="{{id}}"
  data-degree-title="{{title}}" data-degree-start="{{start}}">
  <div class="col s12">
    <h5>{{title}} ({{start}} Start)</h5>
  </div>
</div>

{{#each years}}
  <div class="row" data-degree-year-id="{{yearId}}">
    <div class="col s12">
      <h6>Year {{year}}</h6>
    </div>
    <div class="col s12 m3 degree-season">
      <div class="card">
        <div class="degree-season-heading valign-wrapper brown lighten-3">
          <h6 class="valign">FALL</h6>
        </div>
        <div class="card-content clearfix degree-season-content no-padding">
          {{#if @root.admin}}
            <div class="col s12 admin-degree-course-form">
              <div class="input-field col s12">
                <input class="validate admin-new-course-autocomplete col s9 m8"
                  id="" type="text" placeholder="Course">
                <a href="" class="admin-course-add-btn col s3 m4 btn waves-effect waves-light green darken-4">
                  <i class="material-icons">add</i>
                </a>
              </div>
            </div>
            <ul class="degree-season-courses clearfix" data-quarter="fall">
              {{#each fall}}
                {{#if this}}
                  <li class="degree-season-course" data-course-number="{{this}}">
                    <div class="valign-wrapper admin-degree-course">
                      <i class="material-icons valign"><a href="">reorder</a></i>
                      <div class="center-align admin-course-link valign">
                        <a href="#course" class="degree-course-modal">{{this}}</a>
                      </div>
                      <i class="material-icons valign admin-course-delete-btn"><a href="">clear</a></i>
                    </div>
                  </li>
                {{/if}}
              {{/each}}
            </ul>
          {{else}}
            <ul class="degree-season-courses clearfix" data-quarter="fall">
              {{#each fall}}
                <li class="degree-season-course" data-course-number="{{this}}">
                  <div class="valign-wrapper degree-course">
                    <a href="#course" class="degree-course-modal">{{this}}</a>
                  </div>
                </li>
              {{/each}}
            </ul>
          {{/if}}
        </div>
      </div>
    </div>
    <div class="col s12 m3 degree-season">
